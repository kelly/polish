<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  </head>
  <body leftmargin="0" marginwidth="0" topmargin="0" marginheight="0" offset="0">
    <center>
      <table border="0" cellpadding="0" cellspacing="0" height="100%" width="100%" id="background-table">
        <tr>
          <td align="center" valign="top">
            <!-- pre-header begin -->
            {{#if preHeader}}
            <table border="0" cellpadding="10" cellspacing="0" width="600" id="template-pre-header">
              <tr>
                <td valign="top" class="pre-header-content">      
                  <table border="0" cellpadding="10" cellspacing="0" width="100%">
                    <tr>
                      <td valign="top">
                        {{{preHeader}}}
                      </td>
                    </tr>
                  </table>  
                </td>
              </tr>
            </table>
            {{/if}}
            <!-- pre-header end -->
            <table border="0" cellpadding="0" cellspacing="0" width="600" id="template-container">
              {{#if header}}
              <!-- header begin -->
              <tr>
                <td align="center" valign="top" id="template-header">
                  {{{header}}}
                </td>
              </tr>
              {{/if}}
              <!-- header end -->
              <!-- content begin -->
              {{#if content}}
              <tr>
                <td align="center" valign="top">
                  <table border="0" cellpadding="0" cellspacing="0" width="600" id="template-body">
                    <tr>
                      <td valign="top" class="bodyContent">
                        <table border="0" cellpadding="20" cellspacing="0" width="100%">
                        {{{content}}}
                        </table>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
              {{/if}}
              <!-- content end -->
              <!-- footer start -->
              {{#if footer}}
              <tr>
                <td align="center" valign="top">
                  <table border="0" cellpadding="10" cellspacing="0" width="600" id="template-footer">
                    <tr>
                      <td valign="top" class="footer-content">
                        <table border="0" cellpadding="5" cellspacing="0" width="100%">
                          {{{footer}}}
                        </table>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
              {{/if}}
              <!-- footer end -->
            </table><br />
          </td>
        </tr>
      </table>
    </center>
  </body>
</html>